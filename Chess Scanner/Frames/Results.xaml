<Page
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Chess_Scanner.Frames"
      xmlns:System="clr-namespace:System;assembly=mscorlib" xmlns:Text="clr-namespace:System.Text;assembly=mscorlib" x:Class="Chess_Scanner.Frames.Results"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Results" Loaded="Page_Loaded">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400*"/>
            <ColumnDefinition Width="400*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="List_Box1" FontSize="18" HorizontalContentAlignment="Center" ScrollViewer.VerticalScrollBarVisibility="Hidden" TextElement.FontStyle="Normal" Grid.Column="0"></ListBox>
        <ListBox x:Name="List_Box2" FontSize="18" HorizontalContentAlignment="Center" ScrollViewer.VerticalScrollBarVisibility="Hidden" TextElement.FontStyle="Normal" Grid.Column="1"></ListBox>
    </Grid>
</Page>
